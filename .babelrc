{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["transform-define", "./env-config.js"],
    ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
  ],
  "env": {
    "production": {
      "plutins": [
        ["styled-components", { "ssr": true, "displayName": false, "minify": true, "preprocess": false } ]
      ]
    },
  }
}
